kv=-0.9946 ;
ku=0.9946;
kr=6;
// three design gains for the voltage loop
// bw=1 slow
// bw= 2 medium
// bw=3 fast
// Voltage loop feedback gains 
//Voltage Controller
///Q=diag([1 1 1 1 1])
///r=2000000

Kv1=0.173699671106417 ;
Kv2=0.168748093394050 ;
Kv3=0.016999643012784;
Kv4=0.024445795550324;
Kv5=-0.025272466527808;
kw=6;

vref = 0;
vc = 0;

