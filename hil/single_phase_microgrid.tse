TWOPI = 6.28318530718;
SQRT2 = 1.41421356237;
theta0 = 0;
IN_V_REF =0;
/*End code section*/"
                input_terminals = "real IN_IL1;real IN_VC;real IN_V_DROOP;real IN_W_DROOP;real IN_IL2;"
