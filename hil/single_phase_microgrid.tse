    executeDroop(&droop, IN_V_DIR, IN_V_QUAD, IN_I_DIR, IN_I_QUAD, &v, &p, &q);
    
    // Output values
    OUT_V = v;
    OUT_PF = p;
    OUT_QF = q;
    OUT_REF_P = p_ref;
    OUT_REF_Q = q_ref;
}else{
    // Output values
    OUT_V = 0;
    OUT_PF = 0;
    OUT_QF = 0;
    OUT_REF_P = 0;
    OUT_REF_Q = 0;
}
